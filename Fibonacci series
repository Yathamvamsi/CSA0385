#include<stdio.h>
int main()
{
    int i,a=0,b=1,c=a+b,n;
    printf("enter the n value");
    scanf("%d",&n);
    if(n>0)
    {
        printf("%d",a);
    }
    if(n>1)
    {
        printf("%d",b);
    }
    for(i=2;i<n;i++)
    {
        printf("%d ",c);
        a=b;
        b=c;
        c=a+b;
    }
    printf("\n");
}
